{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "no-signal",
  "version": "0.1.0",
  "identifier": "com.github.auribuo.nosignal",
  "build": {
    "frontendDist": "../src"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "no-signal",
        "width": 1000,
        "height": 800
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
        "icons/logo.svg"
    ]
  }
}
